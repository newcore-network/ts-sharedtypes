{
  "name": "@newcore-network/ts-sharedtypes",
  "version": "0.1.20",
  "description": "Shared types and interfaces for Newcore projects",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "upLib": "tsc && git push origin master && npm publish --access public"
  },
  "keywords": [
    "typescript",
    "shared",
    "types",
    "interfaces",
    "newcore"
  ],
  "author": "Newcore Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
