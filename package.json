{
  "name": "@newcore-network/ts-sharedtypes",
  "version": "0.1.22",
  "description": "Shared types and interfaces for Newcore projects",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./character/enums": {
      "import": "./dist/character/enums.js",
      "require": "./dist/character/enums.js",
      "types": "./dist/character/enums.d.ts"
    },
    "./character/interfaces": {
      "import": "./dist/character/interfaces.js",
      "require": "./dist/character/interfaces.js",
      "types": "./dist/character/interfaces.d.ts"
    },
    "./character/paramTypes": {
      "import": "./dist/character/paramTypes.js",
      "require": "./dist/character/paramTypes.js",
      "types": "./dist/character/paramTypes.d.ts"
    },
    "./utils/events": {
      "import": "./dist/utils/events.js",
      "require": "./dist/utils/events.js",
      "types": "./dist/utils/events.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "upLib": "tsc && git push origin master && npm publish --access public"
  },
  "keywords": [
    "typescript",
    "shared",
    "types",
    "interfaces",
    "newcore"
  ],
  "author": "Newcore Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
